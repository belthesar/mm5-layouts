        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "checklist.html"
      },
      {
        "name": "total",
        "title": "Total",
        "width": 2,
        "headerColor": "#2d4e8a",
        "file": "total.html"
      },
      {
        "name": "edit-total",
        "title": "Edit Total",
        "width": 2,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "save",
            "type": "confirm"
          },
          {
            "name": "cancel",
            "type": "dismiss"
          }
        ],
        "file": "dialogs/edit-total.html"
      },
      {
        "name": "twitter",
        "title": "Twitter",
        "width": 4,
        "headerColor": "#55acee",
        "file": "twitter.html"
      },
      {
        "name": "interview",
        "title": "Interview",
        "width": 2,
        "headerColor": "#2d4e8a",
        "file": "interview.html"
      }
    ],
    "graphics": [
