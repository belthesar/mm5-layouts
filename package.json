  "homepage": "http://gamesdonequick.com/",
  "license": "Apache-2.0",
  "nodecg": {
    "compatibleRange": "~0.8.0",
    "dashboardPanels": [
      {
        "name": "schedule",
        "title": "Schedule",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "schedule/schedule.html"
      },
      {
        "name": "edit-run",
        "title": "",
        "width": 6,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "cancel",
            "type": "dismiss"
          }
        ],
        "file": "schedule/dialogs/edit-run.html"
      },
      {
        "name": "advertisements",
        "title": "Advertisements",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "advertisements.html"
      },
      {
        "name": "checklist",
